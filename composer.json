{
    "name": "wearesho-team/yii2-wearesho-notifications-repository",
    "type": "library",
    "license": "Proprietary",
    "authors": [
        {
            "name": "Beton",
            "email": "aleksa.yagich@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.2",
        "yiisoft/yii2": "2.0.15.1",
        "yiisoft/yii2-queue": "^2.1",
        "wearesho-team/wearesho-notifications-repository": "^1.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.3",
        "phpunit/phpunit": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "Wearesho\\Notifications\\Yii\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wearesho\\Notifications\\Yii\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "lint": "./vendor/bin/phpcs --standard=PSR2 ./src ./tests",
        "phpcbf": "./vendor/bin/phpcbf --standard=PSR2 ./src ./tests",
        "test": "./vendor/bin/phpunit"
    },
    "repositories": [
        {
            "type": "git",
            "url": "git@github.com:wearesho-team/wearesho-notifications-repository.git"
        }
    ]
}
